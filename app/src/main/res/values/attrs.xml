<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Divider">
        <attr name="dividerStrokeWidth" format="integer" />
        <attr name="dividerColor" format="color" />
        <attr name="dividerOrientation" format="enum">
            <enum name="vertical" value="1" />
            <enum name="horizontal" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SquareLayout">
        <attr name="android:layout_gravity" />
        <attr name="layout_position" format="enum">
            <enum name="TOP_START" value="1" />
            <enum name="TOP_END" value="2" />
            <enum name="BOTTOM_END" value="3" />
            <enum name="BOTTOM_START" value="4" />
        </attr>
    </declare-styleable>

